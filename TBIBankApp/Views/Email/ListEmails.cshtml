@model TBIBankApp.Models.Emails.EmailListModel;

@{
    ViewData["Title"] = "Index";
}
<style>
    table {
        font-family: arial, sans-serif;
        border-collapse: collapse;
        width: 100%;
    }

    td, th {
        border: 1px solid #dddddd;
        text-align: left;
        padding: 8px;
    }

    tr:nth-child(even) {
        background-color: #dddddd;
    }
</style>

<div class="cm-md-12">

    <table>
        <tr>
            <th>Email ID</th>
            <th>Recieved</th>
            <th>Sender</th>
            <th>Subject</th>
            <th>Body</th>
            <th>Attachments</th>
            <th>Status</th>
            <th>Change status</th>
        </tr>
        @foreach (var item in Model.EmailViewModels)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.RegisteredInDataBase</td>
                <td>@item.Sender</td>
                <td>@item.Subject</td>
                <td>
                    <button type="submit" class="btn btn-primary">View Body</button>
                </td>
                <td><img src="data:image/jpeg;base64," height="42" width="42">#Attachmants</td>
                <td>Not reviewed</td>
                <td>
                    <button type="submit" class="btn btn-primary">New</button>
                    <button type="submit" class="btn btn-primary">Close</button>
                </tD>

            </tr>
           
        }
    </table>
</div>
<br />
<br />
<nav aria-label="Page navigation example">
    <ul class="pagination">

        <a asp-controller="Email" asp-action="ListEmails" asp-route-Id="@Model.PreviousPage" button type="submit" class="btn btn-primary">
            Previous
        </a>

        <li class="page-item"><a class="page-link" href="#">@Model.CurrentPage</a></li>
        <a asp-controller="Email" asp-action="ListEmails" asp-route-Id="@Model.NextPage" button type="submit" class="btn btn-primary">Next</a>
    </ul>
</nav>
